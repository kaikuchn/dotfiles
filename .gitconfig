[core]
        excludesfile = /Users/kaikuchn/.global_gitignore
        editor = vi
	attributesfile = /Users/kaikuchn/.gitattributes
[user]
        name = Kai Kuchenbecker
        email = kuchenbecker.k@gmail.com
  	signingkey = ssh-rsa 
[alias]
        co = checkout
        st = status
        ci = commit --verbose
        who = config --global user.email
	sf = diff-tree --no-commit-id --name-only -r
	cb = fetch --prune origin
[push]
        default = upstream
	gpgSign = if-asked
	autoSetupRemote = true
[github]
        user = kaikuchn
[commit]
        template = ~/.gitmessage
	verbose = true
	gpgSign = true
[tag]
	gpgSign = true
[pull]
        rebase = true
[init]
	defaultBranch = main
[gpg]
	format = ssh
  
[gpg "ssh"]
	program = "/Applications/1Password.app/Contents/MacOS/op-ssh-sign"
	allowedSignersFile = "~/.ssh/git.allowedSigners"

[log]
	showSignature = true
